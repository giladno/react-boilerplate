{
    "name": "react-boilerplate",
    "version": "1.0.0",
    "description": "Simple react boilerplate",
    "scripts": {
        "start": "parcel index.html --port 3000 --open",
        "build": "npm run clean && parcel build index.html --public-url '.' --no-source-maps --no-content-hash",
        "clean": "rm -rf ./dist"
    },
    "keywords": [],
    "author": "Gilad Novik <gilad@novik.ca>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/giladno/react-boilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/giladno/react-boilerplate/issues"
    },
    "homepage": "https://github.com/giladno/react-boilerplate#readme",
    "dependencies": {
        "normalize.css": "^8.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-react": "^7.19.0",
        "less": "^3.11.1",
        "parcel-bundler": "^1.12.4"
    }
}
